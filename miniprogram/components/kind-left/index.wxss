
.kind-left{
  width: 100%;
  height: 100vh;
  align-self: center;
  justify-self: center;
  background-color: #DCDCDC;
  border-radius: 20rpx;
 
}
.left-item{
  display: flex;
  flex-direction: column;

  margin-bottom: 20rpx;
  position: relative;
  padding:5rpx;
  align-items: center;

}
.left-line{
  width: 7rpx;
  height: 60rpx;
  background-color: red;
  position: absolute;
  left: 0;
  top: 30rpx;
}
.images{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-left: 8rpx;

}
.itemName{
  font-size: 9pt;
  margin-left: 8rpx;
}
.choose{
  background-color: white;

  color: red;
  width: 100%;
}
.bottomLast{
  margin-bottom: 200rpx;
}